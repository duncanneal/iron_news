<!DOCTYPE html>
<html>
<head>
 <link rel="stylesheet" type="text/css" href="reset.css">
 <link rel="stylesheet" type="text/css" href="style.css">
 <title> <%= @page_title %> </title>
</head>
<body>
  <div id="main-wrapper"> 
    <header> 
      <ul>
        <span class="logo">i</span>
        <% @data["titles"].each do |title| %>
        <li><%= title %></li>
        <% end %>
        <h4>login</h4>
      </ul>
    </header>
    <ol>
      <% @data["entry"].each do |entry| %>
      <li>
        <img src="images/grayarrow.gif" alt="arrow_mark">
        <h3><%= entry["headline"] %></h3>
        <h5><%= entry["source"] %></h5>
        <p><%= entry["subtitle"] %></p>
      </li>
      <% end %>
    </ol>
    <h2> More</h2>
  </div> 
  <footer>
    <ul class="footer_element">
      <% @data["footer_list"].each do |item| %>
      <li> 
      <%= item %>
      </li>
      <% end %>
    </ul>
  </footer>
</body>
</html>